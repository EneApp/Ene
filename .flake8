[flake8]
exclude =
    # Do not track .git dir
    .git,
    # Do not track virtualenv dir
    venv,
    # Ignore folders in gitignore
    dist,
    build,
    # Do not track pycache dirs
    __pycache__,
    .tox,
    .idea,
    pyside-setup,
    ene/lib,
    /**/__init__.py,
    ene/constants.py,
    ene/resources,

max-line-length = 99
ignore = E402 E731
